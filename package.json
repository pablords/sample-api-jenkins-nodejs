{
  "name": "jenkins-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:coverage": "jest && npx lite-server",
    "test:watch": "jest --watch",
    "test": "jest",
    "test:unit": "jest ./__test__/unit/**",
    "test:integration": "jest ./__test__/integration/** --detectOpenHandles",
    "start": "node -r dotenv/config ./build/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config --watch --respawn --transpile-only --ignore-watch node_modules ./src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files --no-copy-ignored"
  },
  "dependencies": {
    "@octokit/webhooks": "^9.23.0",
    "@types/jenkins": "^0.23.3",
    "cors": "^2.8.5",
    "date-fns": "^2.24.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jenkins": "^0.28.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/xml2js": "^0.4.9",
    "babel-jest": "^27.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "jest": "^27.3.0",
    "lite-server": "^2.6.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
